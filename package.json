    "socket.io-client": "^3.1.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build:development": "env-cmd -f .env.development react-scripts build",
    "build:staging": "env-cmd -f .env.staging react-scripts build",
    "build:offlineprod_linux": "GENERATE_SOURCEMAP=false env-cmd -f .env.offlineprod react-scripts build",
    "build:offlineprod_win": "set \"GENERATE_SOURCEMAP=false\" && env-cmd -f .env.offlineprod react-scripts build",
    "build:production": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js --ext .jsx ./src",
    "lint-fix": "eslint --ext .js --ext .jsx --fix ./src",
    "lint-output": "eslint --ext .js --ext .jsx -o ./lint-output.txt ./src"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-sass": "^4.14.1",
    "source-map-explorer": "^2.5.2",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
